// To be read by the webapp
// This is similar in spirit to the settings of a visual webapp as they are stored in a Dataiku project (see EDITABLE > web_apps > IJvCkG8.json)
{
    "config": {
        "input_dataset": "companies_joined_prepared",
        "schema": {
            "match_id": {
                "type": "key",
                "editable": false
            },
            "ref_id": {
                "type": "default",
                "editable": false
            },
            "ext_id": {
                "type": "linked_record",
                "linked_ds": "companies_ext",
                "linked_ds_key_col": "id",
                "editable": true
            },
            "ext_name": {
                "type": "lookup_column",
                "linked_record_col": "ext_id",
                "linked_ds_lookup_col": "name",
                "editable": false
            }
        }
    }
}