{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "./python-lib",
                "DKU_CURRENT_PROJECT_KEY": "CRM_PELLETS",
                "ORIGINAL_DATASET": "datamodel_customers"
            },
        },
        {
            "name": "Webapp",
            "cwd": "${workspaceFolder}/webapps/edit-dataset-records/",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "backend.py",
                "FLASK_DEBUG": "1",
                "PYTHONPATH": "../../python-lib",
                "DKU_CURRENT_PROJECT_KEY": "ENTITY_RESOLUTION",
                "ORIGINAL_DATASET": "matches_to_be_reviewed"
            },
            "args": [
                "run",
                "--no-debugger", // this is used to disable the flask built-in debugger so that it doesn't interfere with the VS Code debugger - see https://flask.palletsprojects.com/en/2.2.x/debugging/
                "--port",
                "8000"
            ],
            "jinja": true,
            "justMyCode": false
        }
    ]
}