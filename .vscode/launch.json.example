{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Dash Component Generator",
            "cwd": "${workspaceFolder}/dash_tabulator/",
            "type": "python",
            "request": "launch",
            "module": "dash.development.component_generator",
            "args": [
                "src/lib/components",
                "dash_tabulator",
                "-p",
                "package-info.json"
            ],
            "justMyCode": false
        },
        {
            "name": "Webapp",
            "cwd": "${workspaceFolder}/plugin-webapp-data/webapps/data-editing/",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "backend.py",
                "FLASK_ENV": "development",
                "PYTHONPATH": "../../python-lib",
                "DKU_CURRENT_PROJECT_KEY": "REPLACE_ME",
                "ORIGINAL_DATASET": "replace_me"
            },
            "args": [
                "run",
                "--no-debugger" // this is used to disable the flask built-in debugger so that it doesn't interfere with the VS Code debugger - see https://flask.palletsprojects.com/en/2.2.x/debugging/
            ],
            "jinja": true,
            "justMyCode": false
        }
    ]
}