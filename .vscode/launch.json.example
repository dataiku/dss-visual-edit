{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Dash Component Generator",
            "cwd": "${workspaceFolder}/dash_tabulator/",
            "type": "python",
            "request": "launch",
            "module": "dash.development.component_generator",
            "args": [
                "src/lib/components",
                "dash_tabulator",
                "-p",
                "package-info.json"
            ],
            "justMyCode": false
        },
        {
            "name": "Webapp",
            "cwd": "${workspaceFolder}/plugin-editable-via-webapp/webapps/edit-dataset-records/",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "backend.py",
                "FLASK_ENV": "development",
                "PYTHONPATH": "../../python-lib",
                "DKU_CURRENT_PROJECT_KEY": "WEBAPP_LOUIS",
                // choose from CATEGORIZE_TRANSACTIONS, JOIN_COMPANIES, STAKEHOLDER_OWNERSHIP, EDIT
                "ORIGINAL_DATASET": "test_dataset",
                // choose from transactions_missing, matches, stakeholders_tbc_prepared, demand_stream_prepared_filtered, equipment_shifts_dates, new_parts_copy
            },
            "args": [
                "run",
                "--no-debugger" // this is used to disable the flask built-in debugger so that it doesn't interfere with the VS Code debugger - see https://flask.palletsprojects.com/en/2.2.x/debugging/
            ],
            "jinja": true,
            "justMyCode": false
        }
    ]
}