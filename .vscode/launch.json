    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Module",
            "cwd": "${workspaceFolder}/dash_tabulator/",
            "type": "python",
            "request": "launch",
            "module": "dash.development.component_generator",
            "args": ["src/lib/components", "dash_tabulator", "-p", "package-info.json"],
            "justMyCode": false
        },
        {
            "name": "Python: Flask",
            "cwd": "${workspaceFolder}/plugin-editable-via-webapp/webapps/edit-dataset-records/",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "backend.py",
                "FLASK_ENV": "development",
                "PYTHONPATH": "../../python-lib",
                "DKU_CURRENT_PROJECT_KEY": "BOEING", // choose from CATEGORIZE_TRANSACTIONS, JOIN_COMPANIES, STAKEHOLDER_OWNERSHIP, EDIT
                "ORIGINAL_DATASET": "equipment_shifts_dates", // choose from transactions_missing, matches, stakeholders_tbc_prepared, demand_stream_prepared_filtered, equipment_shifts_dates, new_parts_copy
            },
            "args": [
                "run",
                "--no-debugger"
            ],
            "jinja": true,
            "justMyCode": false
        }
    ]
}